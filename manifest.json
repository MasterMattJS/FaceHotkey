{
    "name": "Pony Face Hotkeys",
    "version": "1.0.0",
    "description": "Easy control of your face pony with your keyboard!",
    "icons": {
        "128": "icon.png"
    },
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "https://pony.town/*",
                "http://pony.town/*"
            ],
            "js": [
                "faceHotkeys.js"
            ]
        }
    ]
}